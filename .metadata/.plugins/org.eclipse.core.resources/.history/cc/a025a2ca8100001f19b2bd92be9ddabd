#include "stm32f446xx.h"


int main(void){

	RCC->AHB1ENR |= (1<<0);

	RCC->APB2ENR |= (1<<0);

	GPIOA->MODER |= (1<<10);

	GPIOA->MODER &= ~(1<<11);

	TIM1->PSC = 3999+1;

	TIM1->ARR = 15999+1;

	TIM1->CR1 |= (1<<0);


	while(1){

		while(!(TIM1->SR & (1<<0))){}
		TIM1->SR &= ~(1<<0);
		GPIOA->ODR ^= (1<<5);



	}
}


